{
  "hosting": {
    "public": "build",
    "rewrites": [
      {
        "source": "/api/paypalPayment",
        "function": "paypalPayment"
      },
      {
        "source": "/api/paypalCashout",
        "function": "paypalCashout"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
